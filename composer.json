    "require": {
        "php": ">=5.4",
        "symfony/yaml": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/filesystem": "^4.3",
        "symfony/config": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3 || ^6.0",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {

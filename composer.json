{
    "name": "theiconic/fixtures",
    "description": "Load fixtures into database from different formats: Yaml, XML, etc.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "THE ICONIC ENGINEERING TEAM",
            "email": "engineering@theiconic.com.au"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-redis": "*",
        "symfony/yaml": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/filesystem": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3 || ^6.0",
        "php-coveralls/php-coveralls": "^v2.7.0"
    },
    "autoload": {
        "psr-4": {
            "TheIconic\\Fixtures\\": "src/",
            "TheIconic\\Fixtures\\Test\\":"tests/TheIconic/Fixtures/"
        }
    }
}
